/**app.wxss**/
@import '/miniprogram_npm/@vant/weapp/common/index.wxss';

page {
  box-sizing: border-box;
  min-height: 100vh;
  padding: 0 0 100rpx;
  color: #323233;
  font-size: 16px;
  background: #f7f8fa;
  -webkit-font-smoothing: antialiased;
}

.container {
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0rpx 0rpx 20rpx 0rpx;
  box-sizing: border-box;
}

.margin-left {
  margin-left: 10px;
}

.margin-right {
  margin-right: 10px;
}

.margin-bottom {
  display: block;
  margin-bottom: 15px;
}
